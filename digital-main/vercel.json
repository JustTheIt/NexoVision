{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1",
        "headers": {
          "Cache-Control": "s-maxage=31536000, immutable"
        }
      },
      {
        "src": "/.*",
        "dest": "/dist/index.html"
      }
    ]
  }